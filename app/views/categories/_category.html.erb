<li>
  <%= link_to category.name, category %>
  <p class="my-margin-right">
    <%= category.created_at.to_time.strftime('%B %e %l:%M %p') %>
  </p>
</li>
<div class="progress">
  <div class="progress-bar" role="progressbar" aria-valuenow="70"
    aria-valuemin="0" aria-valuemax="100" style="width:70%"> <%= t :progress %>
  </div>
</div>
